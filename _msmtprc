account gmail
host smtp.gmail.com
port 587
protocol smtp
auth on
from Samantha E
user samantha.ernano@gmail.com
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mail/.passwords/gmail.gpg"
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

account acorn
host smtp.gmail.com
port 587
protocol smtp
auth on
from Samantha E
user sam@acorncommunity.org
passwordeval "gpg --quiet --for-your-eyes-only --no-tty --decrypt ~/.mail/.passwords/acorn.gpg"
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
